<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<display-name>KLVNBatchWeb</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
<servlet>
<servlet-name>KLVNJobController</servlet-name>
<servlet-class>ml.kelvinco.java.servlet.KLVNJobController</servlet-class>
</servlet>

<servlet>
<servlet-name>KLVNJobStatus</servlet-name>
<servlet-class>ml.kelvinco.java.servlet.KLVNJobStatus</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>KLVNJobController</servlet-name>
<url-pattern>JobController</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>KLVNJobStatus</servlet-name>
<url-pattern>JobStatus</url-pattern>
</servlet-mapping>


</web-app>
